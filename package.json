{
  "name": "wp-plugin",
  "version": "1.0.0",
  "description": "My first WordPress plugin!",
  "author": "Simon",
  "license": "ISC",
  "private": true,
  "dependencies": {
    "@babel/core": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-react": "^7.0.0",
    "@wordpress/block-editor": "^3.2.1",
    "@wordpress/components": "^8.3.1",
    "@wordpress/edit-post": "^3.8.1",
    "@wordpress/plugins": "^2.7.1",
    "@wordpress/scripts": "^5.0.1",
    "accessible-autocomplete": "^1.6.2",
    "babel-loader": "^8.0.6",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9"
  },
  "scripts": {
    "eslint": "wp-scripts lint-js --ext .js,.jsx",
    "watch": "wp-scripts start"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "eslintConfig": {
    "extends": "./node_modules/@wordpress/scripts/config/.eslintrc.js",
    "rules": {
      "arrow-parens": "off",
      "no-console": "off",
      "no-multiple-empty-lines": [
        "error",
        {
          "max": 2
        }
      ],
      "no-unused-vars": "warn",
      "padded-blocks": "off"
    }
  }
}
